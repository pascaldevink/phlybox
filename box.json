{
    "chmod": "0755",
    "banner": "Phlybox",
    "blacklist": [
        "box.json",
        "box.phar",
        "composer.json",
        "composer.lock",
        "composer.phar",
        "phlybox.db",
        "phlybox.phar",
        "phlybox.yml"
    ],
    "directories": [
        "src"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor"
        }
    ],
    "finder-bin": [
        {
            "name": "*.pem",
            "in": "vendor"
        },
        {
            "name": "*.md5",
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "bin/phlybox",
    "output": "phlybox.phar",
    "stub": true
}